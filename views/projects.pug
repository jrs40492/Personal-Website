#projects.section(data-section-name="projects-section")
  #projects_carousel.carousel.slide
    ol.carousel-indicators
      li.active(data-slide-to="0",data-target="#projects_carousel")
      li(data-slide-to="1",data-target="#projects_carousel")
      li(data-slide-to="2",data-target="#projects_carousel")
      li(data-slide-to="3",data-target="#projects_carousel")
    .carousel-inner
      include projects/jacobrswanson.pug
      include projects/ufsports.pug
      include projects/59lucylane.pug
      include projects/blackjack.pug
    a.carousel-control-prev.carousel-arrow(href="#projects_carousel",role="button",data-slide="prev")
      span.carousel-control-prev-icon(aria-hidden="true")
      span.sr-only Previous
    a.carousel-control-next.carousel-arrow(href="#projects_carousel",role="button",data-slide="next")
      span.carousel-control-next-icon(aria-hidden="true")
      span.sr-only Next
